{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["js"],
        "exclude": ["js/layers/menu - 副本.js"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
        "template": "./jsdoc/templates/mytheme",
        "destination": "./jsdoc/",
        "recurse": "true"
    }
}
